        "@sveltejs/kit": "^1.17.0",
        "dayjs": "^1.11.7",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "google-play-scraper": "^9.1.1",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.0",
        "sass": "^1.62.1",
        "svelte": "^3.59.1",
        "svelte-check": "^3.3.2",
