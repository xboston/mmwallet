        "dayjs": "^1.11.7",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "google-play-scraper": "^9.1.1",
        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0",
        "sass": "^1.59.2",
        "svelte": "^3.56.0",
        "svelte-check": "^3.1.2",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
